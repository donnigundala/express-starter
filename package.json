{
  "name": "express-ts-starter",
  "version": "1.0.0",
  "description": "Express & Typescript Starter",
  "main": "server.ts",
  "scripts": {
    "start": "npm run build:tsc && cross-env NODE_ENV=production node build/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:tsc": "rm -r build && tsc && tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts server.ts src/",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.7.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.1.3"
  }
}
